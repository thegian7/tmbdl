{
  "name": "tmbdl",
  "version": "1.0.0",
  "description": "Tom Bombadil Programming Language - A LOTR-themed interpreted language",
  "main": "src/interpreter.js",
  "bin": {
    "tmbdl": "./bin/tmbdl.js"
  },
  "scripts": {
    "start": "node bin/tmbdl.js",
    "test": "node --test test/",
    "build": "pkg . --targets node18-linux-x64,node18-macos-x64,node18-win-x64 --output dist/tmbdl"
  },
  "pkg": {
    "scripts": "bin/tmbdl.js",
    "assets": ["src/**/*", "examples/**/*"],
    "outputPath": "dist"
  },
  "keywords": [
    "programming-language",
    "interpreter",
    "lotr",
    "tom-bombadil",
    "compiler"
  ],
  "author": "",
  "license": "MIT",
  "type": "module"
}
